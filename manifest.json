{
  "manifest_version": 3,
  "name": "Append Search Text",
  "version": "1.0",
  "description": "Append the text field to every google search.",
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "permissions": [
    "declarativeNetRequestWithHostAccess", 
    "storage", 
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action" : {
    "default_popup": "content/options.html"
  }
}
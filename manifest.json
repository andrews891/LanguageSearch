{
  "manifest_version": 3,
  "name": "Language Search",
  "version": "1.0",
  "description": "Append the specified \"[Programming Language]\" to every google search.",
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://www.google.com/"
  ]
}